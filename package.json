    "scripts": {
        "ng": "ng",
        "start": "run-p serve:**",
        "build": "ng build --prod",
        "test": "ng test --code-coverage=true",
        "lint": "./node_modules/.bin/nx lint && ng lint",
        "e2e": "ng e2e --prod --no-webdriver-update",
        "ci": "npm run format:check && npm run lint && npm run test && npm run build -- --deploy-url /simbad-client/ --base-href /simbad-client",
        "generate:simbad-cli-api": "npm run generate --prefix libs/simbad-cli-api",
        "generate:simbad-host-api": "npm run generate --prefix libs/simbad-host-api",
        "serve:app": "ng serve --open",
        "serve:mock": "ng serve simbad-dev-server",
        "update-webdrivers": "npm i -D chromedriver --chromedriver-force-download",
        "affected:apps": "./node_modules/.bin/nx affected:apps",
        "affected:libs": "./node_modules/.bin/nx affected:libs",
        "affected:build": "./node_modules/.bin/nx affected:build",
        "affected:e2e": "./node_modules/.bin/nx affected:e2e",
        "affected:test": "./node_modules/.bin/nx affected:test",
        "affected:lint": "./node_modules/.bin/nx affected:lint",
        "affected:dep-graph": "./node_modules/.bin/nx affected:dep-graph",
        "affected": "./node_modules/.bin/nx affected",
        "format": "./node_modules/.bin/nx format:write",
        "format:write": "./node_modules/.bin/nx format:write",
        "format:check": "./node_modules/.bin/nx format:check",
        "update": "ng update @nrwl/schematics",
        "update:check": "ng update",
        "dep-graph": "./node_modules/.bin/nx dep-graph",
        "bundle-analyze": "webpack-bundle-analyzer dist/projects/simbad-client/stats.json --mode static --report webpack-analyze/index.html",
        "generate-bundle-analyze": "run-s build bundle-analyze",
        "compodoc:generate": "compodoc -p projects/simbad-client/tsconfig.app.json --includes ./usage-docs -d docs"
    },
